{
    "name": "pilibili",
    "private": true,
    "version": "0.1.0",
    "scripts": {
        "dev": "vite --mode development",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint --ext src/**/*.ts{,x}",
        "lint-staged": "lint-staged",
        "postinstall": "husky install"
